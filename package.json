{
  "name": "Aug19Tutorials",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest . --testPathIgnorePatterns e2e/ node_modules/",
    "test:e2e": "detox test",
    "lint": "eslint ."
  },
  "dependencies": {
    "react": "16.8.6",
    "react-native": "0.62.3",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-reanimated": "^1.2.0",
    "react-navigation": "^3.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.1",
    "@types/react": "^16.9.1",
    "@types/react-native": "^0.60.4",
    "@types/react-test-renderer": "^16.9.0",
    "babel-jest": "^24.8.0",
    "detox": "^14.0.1",
    "eslint": "^6.1.0",
    "jest": "^24.8.0",
    "jest-fetch-mock": "^2.1.2",
    "metro-react-native-babel-preset": "^0.55.0",
    "react-native-testing-library": "^1.11.1",
    "react-test-renderer": "16.8.6",
    "typescript": "^3.5.3"
  },
  "jest": {
    "preset": "react-native",
    "automock": false,
    "setupFiles": [
      "./setupJest.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/Aug19Tutorials.app",
        "build": "xcodebuild -workspace ios/Aug19Tutorials.xcworkspace -scheme Aug19Tutorials -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    },
    "test-runner": "jest"
  }
}
